<Window x:Class="UnityPatch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="194" Width="386">
    <Grid>
        <Button Name="Select" Content="选择" HorizontalAlignment="Left" Margin="163,8,0,0" VerticalAlignment="Top"
                Width="75" Click="Button_Click" />
        <Label Content="选择unity所在文件夹：" HorizontalAlignment="Left" Margin="27,5,0,0" VerticalAlignment="Top" Width="131" />
        <Label Content="version：" HorizontalAlignment="Left" Margin="27,58,0,0" VerticalAlignment="Top" Width="67" />
        <TextBlock x:Name="UnityPath" HorizontalAlignment="Left" Margin="27,35,0,0" TextWrapping="Wrap"
                   VerticalAlignment="Top" Width="228" Height="18" />
        <TextBlock x:Name="VersionText" HorizontalAlignment="Left" Margin="99,63,0,0" TextWrapping="Wrap"
                   VerticalAlignment="Top" Width="98" Height="20" />
        <Button x:Name="RegistBtn" Content="激活" HorizontalAlignment="Left" Margin="37,115,0,0" VerticalAlignment="Top"
                Width="121" Click="RegistBtn_Click" IsEnabled="False" />

    </Grid>
</Window>